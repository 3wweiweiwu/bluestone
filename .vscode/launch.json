{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Program",
      "program": "${workspaceFolder}/bin/www.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },
    {
      "name": "Launch Command line",
      "program": "${workspaceFolder}/bin/bluestone.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "args": ["start", "./test/sample-project", "--port", "3601"]
    },
    {
      "name": "Launch Command line for wab-bluestone",
      "program": "${workspaceFolder}/bin/bluestone.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "args": ["start", "../wab-bluestone", "--port", "3601"]
    },
    {
      "name": "Launch Record Command line",
      "program": "${workspaceFolder}/bin/bluestone.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "args": ["record", "https://event-analytics.qa.mnubo.org"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug",
      "program": "${workspaceFolder}/bin/www",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/babel-node",
      // "env": {
      //   "BABEL_ENV": "development"
      // },
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ]
    },
    {
      "name": "Debug Mocha",
      "request": "launch",
      "type": "node",
      "program": "${workspaceFolder}/node_modules/.bin/_mocha",
      "stopOnEntry": false,
      "args": ["--compilers", "js:babel-register"],
      "cwd": ".",
      "runtimeExecutable": null,
      "runtimeArgs": ["--nolazy"],
      "env": {
        "NODE_ENV": "development"
      },
      "sourceMaps": true
    }
  ]
}
